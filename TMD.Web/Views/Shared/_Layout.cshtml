@using System.Configuration
@using TMD.Web
<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!-->
<html>
<!--<![endif]-->
<!-- Head BEGIN -->
<head>
    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta content="Home description" name="description">
    <meta content="Home keywords" name="keywords">
    <meta content="keenthemes" name="author">
    <meta property="og:site_name" content="-CUSTOMER VALUE-">
    <meta property="og:title" content="-CUSTOMER VALUE-">
    <meta property="og:description" content="-CUSTOMER VALUE-">
    <meta property="og:type" content="website">
    <meta property="og:image" content="-CUSTOMER VALUE-"><!-- link to image for socio -->
    <meta property="og:url" content="-CUSTOMER VALUE-">
    <link rel="shortcut icon" href="favicon.ico">
    <!-- Fonts START -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700|PT+Sans+Narrow|Source+Sans+Pro:200,300,400,600,700,900&amp;subset=all" rel="stylesheet" type="text/css">
    <!-- Fonts END -->
    <!-- Global styles START -->
    <link href="@Url.Content("~/Content/global/plugins/font-awesome/css/font-awesome.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/global/plugins/bootstrap/css/bootstrap.min.css")" rel="stylesheet">

    <!-- Theme styles START -->
    <link href="~/Content/global/css/components.css" rel="stylesheet">
    <link href="~/Content/frontend/layout/css/style.css" rel="stylesheet">
    @*<link href="~/Content/frontend/pages/css/style-revolution-slider.css" rel="stylesheet">*@<!-- metronic revo slider styles -->
    <link href="~/Content/frontend/layout/css/style-responsive.css" rel="stylesheet">
    <link href="~/Content/frontend/layout/css/themes/blue.css" rel="stylesheet" id="style-color">
    <link href="~/Content/frontend/layout/css/custom.css" rel="stylesheet">
    <link href="~/Content/css/toastr.css" rel="stylesheet" />
    <!-- Theme styles END -->
    <script src="@Url.Content("~/Content/global/plugins/jquery-1.11.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/global/plugins/jquery-migrate-1.2.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/global/plugins/bootstrap/js/bootstrap.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/frontend/layout/scripts/back-to-top.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/frontend/layout/scripts/layout.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/App/ValidateFields.js")"></script>
    <script src="~/Scripts/App/toastr.js"></script>

</head> <!-- Head END --> <!-- Body BEGIN -->
<body class="corporate">
    <div class="pre-header">
        <div class="container">
            <div class="row">
                <!-- BEGIN TOP BAR LEFT PART -->
                <div class="col-md-6 col-sm-6 additional-shop-info">
                    <ul class="list-unstyled list-inline">
                        <li><i class="fa fa-phone"></i><span>+1 456 6717</span></li>
                        <li><i class="fa fa-envelope-o"></i><span>info@innostark.com</span></li>
                    </ul>
                </div> <!-- END TOP BAR LEFT PART --> <!-- BEGIN TOP BAR MENU --> <div class="col-md-6 col-sm-6 additional-nav">
                    <ul class="list-unstyled list-inline pull-right">
                        @if (User.Identity.IsAuthenticated && !User.IsInRole(Utility.MemberRoleName))
                        {
                            <li><a href="@(ConfigurationManager.AppSettings["SiteURL"].ToString())\Account\LogOff">Log Out</a></li>
                            <li><a href="@(ConfigurationManager.AppSettings["SiteURL"].ToString())\Admin\Home">Admin Panel</a></li>


                        }
                        else if (User.Identity.IsAuthenticated && User.IsInRole(Utility.MemberRoleName))
                        {
                            <li><a href="@(ConfigurationManager.AppSettings["SiteURL"].ToString())\Account\LogOff">Log Out</a></li>
                        }
                        else
                        {
                            <li><a href="@(ConfigurationManager.AppSettings["SiteURL"].ToString())\Account\login">Log In</a></li>
                            <li><a href="@(ConfigurationManager.AppSettings["SiteURL"].ToString())\Account\Pricing">Registration</a></li>
                            <li><a href="#">Contact Us</a></li>
                        }
                    </ul>
                </div> <!-- END TOP BAR MENU -->
            </div>
        </div>
    </div> <!-- END TOP BAR --> <!-- BEGIN HEADER -->
    <div class="header">
        <div class="container">
            <a class="site-logo" href="~/Home/Index">
                <img src="~/Images/Web/logo2.png" alt="Logo Frontend">
            </a>
            <a href="javascript:void(0);" class="mobi-toggler">
                <i class="fa fa-bars"></i>
            </a>
            <!-- BEGIN NAVIGATION -->
            <div class="header-navigation pull-right font-transform-inherit">
                <ul>

                    <li class=@( (Session["MenuKey"] != null && Session["MenuKey"].ToString() == "Home") ? "active" : "")>
                        <a class="" style="font-size: 16px" href="~/Home/Index"> Home  </a>
                    </li>
                    <li class="dropdown dropdown-megamenu">
                        <a class="dropdown-toggle" data-toggle="dropdown" data-target="#" href="#"> News  </a>
                    </li>
                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" data-target="#" href="#"> Live Data  </a>
                    </li>
                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" data-target="#" href="#"> Portfolio  </a>
                    </li>
                    <li class=@( (Session["MenuKey"] != null && Session["MenuKey"].ToString() == "Pricing") ? "active" : "")> <a href="~/Account/Pricing"> Pricing  </a></li>
                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" data-target="#" href="#"> About Us  </a>
                    </li>
                </ul>
            </div> <!-- END NAVIGATION -->
        </div>

        <input type="hidden" id="siteURL" value="@( "http://"+Request.Url.Host.ToLower()+"/")" />
    </div> <div class="main" id="main"> @RenderBody() </div> <!-- Header END --> <!-- BEGIN FOOTER --> <div class="footer">
        <div class="container">
            <div class="row">
                <!-- BEGIN COPYRIGHT --> <div class="col-md-6 col-sm-6 padding-top-10"> 2015 ©  InnoSTARK UI. ALL Rights Reserved. <a href="#">Privacy Policy</a> | <a href="#">Terms of Service</a> </div> <!-- END COPYRIGHT --> <!-- BEGIN PAYMENTS -->
                <div class="col-md-6 col-sm-6">
                    <ul class="social-footer list-unstyled list-inline pull-right">
                        <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                        <li><a href="#"><i class="fa fa-google-plus"></i></a></li>
                        <li><a href="#"><i class="fa fa-dribbble"></i></a></li>
                        <li><a href="#"><i class="fa fa-linkedin"></i></a></li>
                        <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                        <li><a href="#"><i class="fa fa-skype"></i></a></li>
                        <li><a href="#"><i class="fa fa-github"></i></a></li>
                        <li><a href="#"><i class="fa fa-youtube"></i></a></li>
                        <li><a href="#"><i class="fa fa-dropbox"></i></a></li>
                    </ul>
                </div> <!-- END PAYMENTS -->
            </div>
        </div>
    </div> <!-- END FOOTER -->  <!-- END RevolutionSlider -->


        <script type="text/javascript">
            jQuery(document).ready(function () {
                Layout.init();
                //    Layout.initOWL();
                //RevosliderInit.initRevoSlider();
                //Layout.initTwitter();

                Layout.initFixHeaderWithPreHeader(); /* Switch On Header Fixing (only if you have pre-header) */
                Layout.initNavScrolling();
            });
        </script>
        <!-- END PAGE LEVEL JAVASCRIPTS -->
    </body>
    <!-- END BODY -->

</html>