@{
    ViewBag.Title = "Batch Import";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<header>

    <h3 class="page-title">
        Batch Import <small>(Ebay Integration Summary)</small>
    </h3>
    @*<h3> <small><a href="~/Account/Create">Add New User</a></small></h3>*@
</header>

<div class="container-fluid">
    <form class="form-horizontal">
        <div class="row-fluid">
            <div class="span12">
                <div class="portlet-title">
                    <div class="caption">
                        <i class="fa fa-filter"></i>Filters
                    </div>
                    <div class="tools">
                        <input type="button" class="btn green" id="btnFltr" value="Filter" />
                        <input type="button" class="btn green titleButton" id="btnResetFltr" value="Reset">
                        <a href="javascript:;" class="collapse">
                        </a>
                    </div>
                </div>
                <div class="portlet-body form">
                    <div class="form-actions">
                        <div>
                            <table class="responsive">
                                <tr>
                                    <td>
                                        <label class="control-label"> Search: </label>
                                    </td>
                                    <td>
                                        <div class="input-icon">
                                            <i class="fa fa-question-circle"></i>
                                            @Html.TextBox("SearchString", "", new { @class = "searchInputs form-control input-circle", @id = "SearchInput", @placeholder = "Activity Name" })
                                        </div>

                                    </td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                </tr>
                            </table>
                        </div>
                    </div>

                </div>
                <div class="portlet box blue">

                    

                </div>
            </div>
        </div>
    </form>
</div>
