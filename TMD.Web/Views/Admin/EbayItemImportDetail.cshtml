@model TMD.Web.Models.StagingEbayItemModel

@{
    ViewBag.Title = "EbayItemImportDetail";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    Session["MenuKey"] = 4;
}

<div class="portlet box blue-hoki">
    <div class="portlet-title">

        <div class="tools">
            @*<a href="javascript:;" class="collapse">
                </a>*@


        </div>
        <div class="caption">
            <i class="fa fa-male"></i>Ebay Item Details
        </div>
        <div class="tools">
            <a href="~/Admin/EbayItemImportLV" style="color:white">
                Cancel
            </a>


        </div>
    </div>

    

        <div class="portlet-body">
            <div class="container-fluid">
                @Html.AntiForgeryToken()
                
              
                <div class="row margin-top-20">
                    <label class="col-md-2 control-label">Item Id</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.EbayItemtId, new { @class = "form-control input-circle", @readonly = "readonly" })
                    </div>

                    <label class="col-md-2 control-label">Batch Import Id</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.EbayBatchImportId, new { @class = "form-control input-circle mandatory", @readonly = "readonly" })
                    </div>


                    <label class="col-md-2 control-label">Toy Grader Id</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.ToyGraderItemId, new { @class = "form-control input-circle mandatory", @readonly = "readonly" })
                    </div>
                </div>

                <div class="row margin-top-10">
                    <label class="col-md-2 control-label">Created By</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.CreatedBy, new { @class = "form-control input-circle", @readonly = "readonly" })
                    </div>

                    <label class="col-md-2 control-label">Created On</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.CreatedOn, new { @class = "form-control input-circle mandatory", @readonly = "readonly" })
                    </div>


                    <label class="col-md-2 control-label">Modified By</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.ModifiedBy, new { @class = "form-control input-circle mandatory", @readonly = "readonly" })
                    </div>
                </div>

                
                <div class="row margin-top-10">
                    <label class="col-md-2 control-label">Modified On</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.ModifiedOn, new { @class = "form-control input-circle", @readonly = "readonly" })
                    </div>

                    <label class="col-md-2 control-label">Deleted</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.Deleted, new { @class = "form-control input-circle mandatory", @readonly = "readonly" })
                    </div>


                    <label class="col-md-2 control-label">Deleted On</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.DeletedOn, new { @class = "form-control input-circle mandatory", @readonly = "readonly" })
                    </div>
                </div>
                
                <div class="row margin-top-10">
                    <label class="col-md-2 control-label">Deleted By</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.DeletedBy, new { @class = "form-control input-circle", @readonly = "readonly" })
                    </div>

                    <label class="col-md-2 control-label">Condition</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.Condition, new { @class = "form-control input-circle mandatory", @readonly = "readonly" })
                    </div>


                    <label class="col-md-2 control-label">Country Code</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.CountryCode, new { @class = "form-control input-circle mandatory", @readonly = "readonly" })
                    </div>
                </div>
                
                <div class="row margin-top-10">
                    <label class="col-md-2 control-label">Global Id</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.GlobalId, new { @class = "form-control input-circle", @readonly = "readonly" })
                    </div>

                    <label class="col-md-2 control-label">Item Id</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.ItemId, new { @class = "form-control input-circle mandatory", @readonly = "readonly" })
                    </div>


                    <label class="col-md-2 control-label">Listing Info Buy It Now Available</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.ListingInfoBuyItNowAvailable, new { @class = "form-control input-circle mandatory", @readonly = "readonly" })
                    </div>
                </div>
                
                
                <div class="row margin-top-10">
                    <label class="col-md-2 control-label">ListingInfo BuyItNowPrice</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.ListingInfoBuyItNowPrice, new { @class = "form-control input-circle", @readonly = "readonly" })
                    </div>

                    <label class="col-md-2 control-label">ListingInfo EndTime</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.ListingInfoEndTime, new { @class = "form-control input-circle mandatory", @readonly = "readonly" })
                    </div>


                    <label class="col-md-2 control-label">ListingInfo Gift</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.ListingInfoGift, new { @class = "form-control input-circle mandatory", @readonly = "readonly" })
                    </div>
                </div>
                
                <div class="row margin-top-10">
                    <label class="col-md-2 control-label">ListingInfo ListingType</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.ListingInfoListingType, new { @class = "form-control input-circle", @readonly = "readonly" })
                    </div>

                    <label class="col-md-2 control-label">ListingInfo StartTime</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.ListingInfoStartTime, new { @class = "form-control input-circle mandatory", @readonly = "readonly" })
                    </div>


                    <label class="col-md-2 control-label">Primary Category</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.PrimaryCategory, new { @class = "form-control input-circle mandatory", @readonly = "readonly" })
                    </div>
                </div>
                
                <div class="row margin-top-10">
                    <label class="col-md-2 control-label">Producit Id</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.ProducitId, new { @class = "form-control input-circle", @readonly = "readonly" })
                    </div>

                    <label class="col-md-2 control-label">Secondary Category</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.SecondaryCategory, new { @class = "form-control input-circle mandatory", @readonly = "readonly" })
                    </div>


                    <label class="col-md-2 control-label">Top Rated Seller </label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.SellerInfoTopRatedSeller, new { @class = "form-control input-circle mandatory", @readonly = "readonly" })
                    </div>
                </div>
                
                
                <div class="row margin-top-10">
                    <label class="col-md-2 control-label">Selling Bid Count</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.SellingStatusBidCount, new { @class = "form-control input-circle", @readonly = "readonly" })
                    </div>

                    <label class="col-md-2 control-label">Selling Current Price</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.SellingStatusCurrentPrice, new { @class = "form-control input-circle mandatory", @readonly = "readonly" })
                    </div>


                    <label class="col-md-2 control-label">Selling State </label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.SellingStatusSellingState, new { @class = "form-control input-circle mandatory", @readonly = "readonly" })
                    </div>
                </div>
                
                
                <div class="row margin-top-10">
