                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.SellingStatusTimeLeft, new { @class = "form-control input-circle", @readonly = "readonly" })
                    </div>

                    <label class="col-md-2 control-label">Store Name</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.StoreInfoStoreName, new { @class = "form-control input-circle mandatory", @readonly = "readonly" })
                    </div>


                    <label class="col-md-2 control-label">Store URL</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.StoreInfoStoreURL, new { @class = "form-control input-circle mandatory", @readonly = "readonly" })
                    </div>
                </div>

                
                <div class="row margin-top-10">
                    <label class="col-md-2 control-label">Sub Title</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.SubTitle, new { @class = "form-control input-circle", @readonly = "readonly" })
                    </div>

                    <label class="col-md-2 control-label">Title</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.Title, new { @class = "form-control input-circle mandatory", @readonly = "readonly" })
                    </div>


                    <label class="col-md-2 control-label">View Item Url</label>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.ViewItemUrl, new { @class = "form-control input-circle mandatory", @readonly = "readonly" })
                    </div>
                </div>

                
                
                
                

                



                

            </div>
        </div>


    
</div>

